{
  "expo": {
    "name": "MWG Partner",
    "slug": "partner",
    "version": "1.0.1",
    "runtimeVersion": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "partner",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/c45e4938-e08b-49aa-8d2c-bcd075a18f30",
      "requestHeaders": {
        "expo-channel-name": "preview"
      }
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.whiteglovesapp.partner",
      "config": {
        "googleMapsApiKey": "${EXPO_PUBLIC_GOOGLE_PLACES_API_KEY}"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to processing the bookings.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We need your location to processing the bookings.",
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "${EXPO_PUBLIC_GOOGLE_PLACES_API_KEY}"
        }
      },
      "adaptiveIcon": {
        "backgroundColor": "#ff0000",
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundImage": "./assets/images/adaptive-icon.png",
        "monochromeImage": "./assets/images/adaptive-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION",
        "android.permission.VIBRATE",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.USE_FULL_SCREEN_INTENT",
        "android.permission.SYSTEM_ALERT_WINDOW"
      ],
      "jsEngine": "hermes",
      "package": "com.whiteglovesapp.partner",
      "googleServicesFile": "./google-services.json",
      "versionCode": 5
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ff0000",
          "dark": {
            "backgroundColor": "#ff0000"
          }
        }
      ],
      [
        "expo-location",
        {
          "isAndroidBackgroundLocationEnabled": false
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 24,
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResources": true,
            "enableMinifyInReleaseBuilds": true
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification.png",
          "color": "#ff5075",
          "defaultChannel": "booking_channel_v2",
          "sounds": ["./assets/sounds/booking.wav"],
          "enableBackgroundRemoteNotifications": false
        }
      ],
      [
        "expo-maps",
        {
          "requestLocationPermission": true,
          "locationPermission": "Allow MWG Partner to use your location"
        }
      ],
      [
        "expo-audio",
        {
          "microphonePermission": "Allow MWG Partner to play sound for new bookings."
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c45e4938-e08b-49aa-8d2c-bcd075a18f30"
      },
      "EXPO_PUBLIC_GOOGLE_PLACES_API_KEY": "${EXPO_PUBLIC_GOOGLE_PLACES_API_KEY}"
    },
    "owner": "whiteglovesapp"
  }
}
